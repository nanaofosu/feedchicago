<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane  prefHeight="600.0" prefWidth="900.0" styleClass="main-wrapper" stylesheets="@../assets/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.HomeController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <VBox prefHeight="600.0" prefWidth="900.0">
         <children>
            <HBox prefHeight="300.0" prefWidth="900.0">
               <children>
                  <Button id="buttonAddSkill" fx:id="addSkillButton" mnemonicParsing="false" styleClass="dashboard-button" text="Add Skill" />
                  <Button id="buttonAddVolunteer" fx:id="addVolunteerButton" mnemonicParsing="false" styleClass="dashboard-button" text="Add Volunteer" />
                  <Button id="buttonViewSkills" fx:id="viewSkillsButton" mnemonicParsing="false" styleClass="dashboard-button" text=" View Skills" />
               </children>
            </HBox>
            <HBox prefHeight="300.0" prefWidth="900.0" styleClass="dashboard-button-wrapper">
               <children>
                  <Button id="buttonViewVolunteers" fx:id="viewVolunteersButton" mnemonicParsing="false" styleClass="dashboard-button" text="View Volunteers" />
                  <Button id="buttonLogout" fx:id="logoutButton" mnemonicParsing="false" styleClass="dashboard-button" text="Logout" />
                  <Button mnemonicParsing="false" styleClass="dashboard-button" text="Button" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
