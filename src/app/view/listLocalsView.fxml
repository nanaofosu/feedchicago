   <children>
      <TableView fx:id="table" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="firstNameCol" prefWidth="116.0" text="First Name" />
          <TableColumn fx:id="lastNameCol" prefWidth="134.0" text="Last Name" />
            <TableColumn fx:id="emailCol" prefWidth="202.0" text="Email Address" />
            <TableColumn fx:id="phoneCol" prefWidth="147.0" text="Phone Number" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
