<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #334052;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.LoginController">
   <children>
      <Label layoutX="396.0" layoutY="39.0" text="Login" textFill="WHITE">
         <font>
            <Font name="Gotham-Book" size="62.0" />
         </font>
      </Label>

      <Label fx:id="loginViewErrorMessage" layoutX="365.0" layoutY="315.0" text="username and or password incorrect" textFill="WHITE" />
      <TextField fx:id="loginViewUsername" layoutX="394.0" layoutY="137.0" promptText="username" style="-fx-background-color: #e0e0e0;" />
      <PasswordField fx:id="loginViewPassword" layoutX="394.0" layoutY="185.0" promptText="password" />
      <Button fx:id="loginViewLoginButton" layoutX="448.0" layoutY="230.0" mnemonicParsing="false" text="Login" />
   </children>
</AnchorPane>
